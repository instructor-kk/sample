    "count = 0\n",
    "student_num = int(input(\"学生の数を入力\"))\n",
    "score_list = list()\n",
    "\n",
    "while count < student_num:\n",
    "    count += 1\n",
    "    score = int(input(f'{count}人目の点数　>>'))\n",
    "    score_list.append(score)\n",
    "\n",
    "print(score_list)\n",
    "total = sum(score_list)\n",
    "\n",
    "print(f'平均点は{total / student_num}点です')\n"
   ]
  }
